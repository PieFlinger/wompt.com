!!! 5
html("xmlns\u003Afb"="http://www.facebook.com/2008/fbml", xmlns="http://www.w3.org/1999/xhtml")
	head
		!=partial('layouts/_head')
		-if(w.css_file)
			link(type='text/css',src=w.css_file)
		style(type='text/css')
			.main #content{
				top: 0px;
			}
			.main #user_list{
				top: 40px;
			}

			#top{
				left:auto;
				width:180px;
			}
			
			#chat #wompt{
				display:inline;
			}
			
			a.popout{
				background:center no-repeat url(/images/popout.png);
				height:20px;
				width:20px;
				display:block;
				position:absolute;
				right:10px;
				top:7px;
			}
			
	body(id=w.page_name, style='background:transparent;')
		#fb-root
		!=partial('layouts/_mini_header')
		.main
			//#bg
			!{body}
			!=partial('layouts/_footer')
			.clear
		- if(w.jquery)
			script(src="https://ajax.googleapis.com/ajax/libs/jquery/1.3.1/jquery.min.js", type="text/javascript")
		- if(w.page_js)
			!{assetHelpers.assetGroupTag(w.page_js, w.user && w.user.doc)}
		- if(w.channel)
			script(type='text/javascript', src='http://connect.facebook.net/en_US/all.js#appId=181725458505189&xfbml=1',async='true')
		!=partial('shared/_ga')
