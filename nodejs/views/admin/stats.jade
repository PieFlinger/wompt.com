table
	tr
		th Active Channels
		td #{w.app.channels.count}
	tr
		th Active Clients
		td #{w.app.clients.count}
	tr
		th Unique Users
		td #{w.app.clients.userCount}		
	tr
		th Popular Channels
		td
			- w.app.popular_channels.rebuildPopularList()
			- each chan in w.app.popular_channels.sorted_list
				div (#{chan.clients.userCount}) - #{chan.name}
	tr
		th App State
		td
			pre	#{JSON.stringify(w.app.appStateMonitor.previousState)}
